{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "node_modules/swagger-ui-dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/docs(?:/|$)(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
